{
  "collectionName": "questions",
  "info": {
    "singularName": "question",
    "pluralName": "questions",
    "displayName": "Question"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "question": {
      "type": "text",
      "required": true
    },
    "university": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::university.university"
    },
    "answered": {
      "type": "boolean",
      "default": false
    }
  }
}
